{
	"name": "frontend",
	"type": "commonjs",
	"version": "0.0.1",
	"scripts": {
		"dev": "bun --bun astro dev --host 0.0.0.0",
		"https-proxy": "caddy run --config Caddyfile",
		"build-and-check": "bun --bun astro check && bun run build",
		"build": " bun --bun astro build",
		"preview": "bun  astro preview",
		"run-server": "bun ./src/server.js",
		"prepare": "cd .. && husky frontend/.husky",
		"codegen": "cd ../backend && tygo generate",
		"temp-run-be": "wsl -e bash -c \"cd ../backend && make watch\""
	},
	"dependencies": {
		"@astrojs/check": "^0.9.4",
		"@astrojs/node": "8.3.4",
		"@astrojs/react": "^3.6.2",
		"@astrojs/tailwind": "5.1.2",
		"@hookform/resolvers": "^3.9.0",
		"@nanostores/react": "^0.8.0",
		"@radix-ui/react-alert-dialog": "^1.1.2",
		"@radix-ui/react-checkbox": "^1.1.2",
		"@radix-ui/react-dialog": "^1.1.2",
		"@radix-ui/react-dropdown-menu": "^2.1.2",
		"@radix-ui/react-label": "^2.1.0",
		"@radix-ui/react-select": "^2.1.2",
		"@radix-ui/react-slot": "^1.1.0",
		"@radix-ui/react-tabs": "^1.1.1",
		"@radix-ui/react-toast": "^1.2.2",
		"@radix-ui/react-tooltip": "^1.1.3",
		"@tanstack/react-table": "^8.20.5",
		"@types/react": "^18.3.11",
		"@types/react-dom": "^18.3.0",
		"astro": "4.16.9",
		"axios": "^1.7.7",
		"canvas-confetti": "^1.9.3",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"express": "^4.21.0",
		"framer-motion": "^11.11.1",
		"mini-svg-data-uri": "^1.4.4",
		"nanostores": "^0.11.3",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-hook-form": "^7.53.0",
		"react-icons": "^5.3.0",
		"recharts": "^2.12.7",
		"tailwind-merge": "^2.5.3",
		"tailwindcss": "^3.4.13",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.6.2",
		"zod": "^3.23.8"
	},
	"trustedDependencies": [
		"astro",
		"tailwindcss"
	],
	"devDependencies": {
		"@types/bun": "^1.1.10",
		"@types/canvas-confetti": "^1.6.4",
		"@types/express": "^5.0.0",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"prettier": "^3.3.3",
		"prettier-plugin-astro": "^0.14.1",
		"prettier-plugin-tailwindcss": "^0.6.8"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,css,scss,md,yaml,yml,mjs}": [
			"bun --bun prettier --write"
		]
	}
}
