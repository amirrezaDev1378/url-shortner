{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"strictNullChecks": true,
		"strict": true,
		"allowJs": true,
		"exactOptionalPropertyTypes": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
			"@UI/*": ["src/components/UI/*"]
		}
	}
}
